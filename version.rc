#include "include\version.h"
#include <winres.h>

1 VERSIONINFO
 FILEVERSION INFODUMPF4_VERSION_MAJOR, INFODUMPF4_VERSION_MINOR, INFODUMPF4_VERSION_PATCH, INFODUMPF4_VERSION_BETA
 PRODUCTVERSION INFODUMPF4_VERSION_MAJOR, INFODUMPF4_VERSION_MINOR, INFODUMPF4_VERSION_PATCH, INFODUMPF4_VERSION_BETA
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "FileDescription", "INFODumpF4"
			VALUE "FileVersion", INFODUMPF4_VERSION_VERSTRING
            VALUE "InternalName", "INFODumpF4"
            VALUE "LegalCopyright", "MIT License"
            VALUE "ProductName", "INFODumpF4"
			VALUE "ProductVersion", INFODUMPF4_VERSION_VERSTRING
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
